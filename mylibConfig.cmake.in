@PACKAGE_INIT@

set(@PROJECT_NAME@_INSTALL_DIR ${CMAKE_CURRENT_LIST_DIR}/../../..)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

set_and_check(@PROJECT_NAME@_INCLUDE_DIRS "${@PROJECT_NAME@_INSTALL_DIR}/include")
